# Application Settings
APP_ENV=development
API_PORT=8000
API_HOST=0.0.0.0

# Supabase Configuration (Vector Store)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key

# Supabase Auth (Admin Operations)
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_JWT_SECRET=your-supabase-jwt-secret
ADMIN_EMAIL=admin@example.com

# Groq API Configuration (comma-separated for key rotation, single key also works)
GROQ_API_KEYS=your-groq-key-1,your-groq-key-2
GROQ_MODEL=openai/gpt-oss-120b
GROQ_MODEL_FAST=openai/gpt-oss-120b

# Google Gemini Configuration (comma-separated for key rotation, single key also works)
GEMINI_API_KEYS=your-gemini-key-1,your-gemini-key-2
GEMINI_MODEL=gemini-pro

# Embedding Configuration (Gemini API)
EMBEDDING_MODEL=gemini
EMBEDDING_DIMENSION=768

# RAG Settings
CHUNK_SIZE=500
CHUNK_OVERLAP=50
MAX_RAG_RESULTS=5
SIMILARITY_THRESHOLD=0.5

# Agent Configuration\
MAX_ROUTER_ITERATIONS=2

# Logging
LOG_LEVEL=INFO

# Opik Configuration
OPIK_API_KEY=your_opik_api_key_here
OPIK_WORKSPACE_NAME=federal-ka-manjan-educational-ai-agent
